<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pawel.test_1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:name="com.pawel.kantor.praca.inzynierska.GlobalSocket"
        android:allowBackup="true"
        android:icon="@drawable/ball"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.Menu"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.Game"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.Options"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.GameOver"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.GameModes"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.pawel.kantor.praca.inzynierska.Bluetooth.BluetoothMainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>